def triangle_type(a, b, c):
    if a <= 0 or b <= 0 or c <= 0:
        return "Не треугольник: стороны должны быть положительными"
    if a + b <= c or a + c <= b or b + c <= a:
        return "Не треугольник: не выполняется неравенство треугольника"
    
    if a == b == c:
        type_by_sides = "равносторонний"
    elif a == b or b == c or a == c:
        type_by_sides = "равнобедренный"
    else:
        type_by_sides = "разносторонний"
def main():
    print("Определение типа треугольника по трём сторонам")
    a = float(input("Введите длину первой стороны: "))
    b = float(input("Введите длину второй стороны: "))
    c = float(input("Введите длину третьей стороны: "))
    result = triangle_type(a, b, c)
    print(f"\nРезультат: {result}")
if __name__ == "__main__":
    main()
